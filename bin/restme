#!/usr/bin/env node
"use strict";
var
	Restme = require("../lib/restme").Restme,
	argv = require("yargs")
		.alias("a", "auth")
		.alias("j", "json")
		.alias("r", "raw")
		.alias("h", "header")
		.alias("f", "form")
		.alias("d", "body")
		.alias("I", "only-response-header")
		.alias("B", "only-response-body")
		.argv;

(new Restme(argv)).init();
